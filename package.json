{
  "name": "mern-forum",
  "version": "1.0.0",
  "description": "A basic forum app made with the MERN stack",
  "main": "app.js",
  "scripts": {
    "frontend-install": "npm install --prefix frontend",
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "dev": "concurrently \"npm run server\" \"npm start --prefix frontend\"",
    "dev:debug": "concurrently \"npm run server:debug\" \"npm start --prefix frontend\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samwalker191/mern-forum.git"
  },
  "author": "Walker",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samwalker191/mern-forum/issues"
  },
  "homepage": "https://github.com/samwalker191/mern-forum#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
